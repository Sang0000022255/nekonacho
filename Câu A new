#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float a,b;
    scanf("%f%f",&a,&b);
    if(a==0)
    {
        if(b==0)
            printf("Countless");
        else 
            printf("None");
    }
    else
        printf("%.2f",(float)-b/a);
        
    
    return 0;
}
